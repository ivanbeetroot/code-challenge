{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/countries-fetch/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/countries-fetch/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/countries-fetch",
  "projects": {
    "countries-fetch": {
      "type": "application",
      "root": "apps/countries-fetch",
      "entryFile": "main",
      "sourceRoot": "apps/countries-fetch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/countries-fetch/tsconfig.app.json",
        "webpack": false
      }
    },
    "countries-api": {
      "type": "application",
      "root": "apps/countries-api",
      "entryFile": "main",
      "sourceRoot": "apps/countries-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/countries-api/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json",
        "webpack": false
      }
    }
  }
}
